\begin{frame}{Example 8}
      \begin{table}[]
\centering

%\label{mtel}
\begin{tabular}{clccc}
\multicolumn{1}{l}{}      & \multicolumn{1}{c}{}               & \multicolumn{2}{c}{Player 2}  &                                  \\
\multirow{5}{*}{Player 1} &                                    & \multicolumn{1}{l}{L} & \multicolumn{1}{l}{M} & \multicolumn{1}{l}{R} \\ \cline{3-5} 
                          & \multicolumn{1}{l|}{U} & \multicolumn{1}{c|}{4,4}        & \multicolumn{1}{c|}{-1,5}
                           & \multicolumn{1}{c|}{3, 3}\\ \cline{3-5} 
                          & \multicolumn{1}{l|}{M}       &
                          \multicolumn{1}{c|}{5,-1}        & \multicolumn{1}{c|}{1,1}
                           & \multicolumn{1}{c|}{0,-1}\\ \cline{3-5}
                           & \multicolumn{1}{l|}{D} & \multicolumn{1}{c|}{4,10}         & \multicolumn{1}{c|}{3,3}   & \multicolumn{1}{c|}{5,7}\\ \cline{3-5} 
\end{tabular}
\end{table}
